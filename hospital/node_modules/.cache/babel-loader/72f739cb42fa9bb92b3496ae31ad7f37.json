og = true;\n      this.Doctor.id = id;\n\n      if (type === 0) {\n        this.Doctor.type = false;\n      } else {\n        this.Doctor.type = true;\n      }\n\n      this.Doctor.address = address;\n    },\n    //修改用户信息\n    editDoctor: function editDoctor() {\n      var _this3 = this;\n\n      console.log(this.Doctor.id);\n\n      if (this.Doctor.type === true) {\n        this.Doctor.type = 1;\n      } else {\n        this.Doctor.type = 0;\n      }\n\n      console.log(this.Doctor);\n      this.$http.post(\"/doctor/update\", this.Doctor).then(function (res) {\n        _this3.editDailog = false;\n\n        _this3.$message.success('修改成功');\n\n        return _this3.getList();\n      });\n    },\n    // 添加医生\n    addDoctorMethod: addDoctorMethod\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAiJA;AACA;AACAA;AACAC;AADA,GADA;AAIAC,MAJA,kBAIA;AACA;AACAC;AACAC,gBADA;AAEAC;AAFA,OADA;AAKAD,cALA;AAMAE,gBANA;AAOA;AACAC;AACAH,gBADA;AAEAI,gBAFA;AAGAC,eAHA;AAIAC,kBAJA;AAKAC,sBALA;AAMAC,mBANA;AAOAC,oBAPA;AAQAC;AARA,OARA;AAkBA;AACAC,mBAnBA;AAoBAC;AACAR,gBADA;AAEAI,mBAFA;AAGAK,cAHA;AAIAC;AACAD;AADA;AAJA,OApBA;AA4BAE,uBA5BA;AA6BAC;AA7BA;AA+BA,GApCA;AAqCAC,SArCA,qBAqCA;AACA;AACA,GAvCA;AAwCAC;AACAC,WADA,qBACA;AAAA;;AACA;AACA;AAEA,OAHA;AAIA,KANA;AAOAC,gBAPA,wBAOAP,EAPA,EAOA;AAAA;;AACA;AACA;AACA;;AACA;AACA,OAHA;AAIA;AACA,KAdA;AAeAQ,oBAfA,4BAeAR,EAfA,EAeAT,IAfA,EAeAI,OAfA,EAeA;AACA;AACA;;AACA;AACA;AACA,OAFA,MAEA;AACA;AACA;;AACA;AACA,KAxBA;AAyBA;AACAc,cA1BA,wBA0BA;AAAA;;AACAC;;AACA;AACA;AACA,OAFA,MAEA;AACA;AACA;;AACAA;AACA;AACA;;AACA;;AACA;AACA,OAJA;AAKA,KAvCA;AAwCA;AACAC;AAzCA;AAxCA","names":["components","adminTop","data","query","name","doctor","radio","addDoctor","type","sex","cardId","department","address","username","password","tableData","Doctor","id","user","editDailog","addDailog","created","methods","getList","deleteDocket","editDailogMethod","editDoctor","console","addDoctorMethod"],"sourceRoot":"src/components/admin","sources":["doctorManage.vue"],"sourcesContent":["<template>\n  <div>\n    <adminTop></adminTop>\n    <main>\n      <el-row>\n        <el-col :span=\"24\">\n          <h3>医生管理</h3>\n        </el-col>\n        <!-- 搜索框 -->\n        <el-col :span=\"15\">\n          <el-form :model=\"query\" label-width=\"80px\">\n            <el-form-item label=\"姓名\" label-width=\"100px\">\n              <el-input\n                v-model=\"query.condition\"\n                placeholder=\"医生姓名\"\n              ></el-input>\n              <el-button type=\"primary\" icon=\"el-icon-search\">搜索</el-button>\n            </el-form-item>\n          </el-form>\n        </el-col>\n        <el-col :span=\"3\" :offset=\"4\">\n          <el-button type=\"primary\" icon=\"el-icon-circle-plus-outline\"  @click=\"addDailog = true\"  >添加</el-button  >\n        </el-col>\n        <!-- 数据 -->\n        <el-table :data=\"tableData\" border style=\"width: 100%\">\n          <el-table-column label=\"序号\" width=\"80px\">\n            <template slot-scope=\"scopes\">\n              {{ scopes.$index + 1 }}\n            </template>\n          </el-table-column>\n          <el-table-column prop=\"name\" label=\"昵称\"></el-table-column>\n          <el-table-column prop=\"type\" label=\"是否为专家\">\n            <template slot-scope=\"scope\">\n              {{ scope.row.type == 0 ? \"非专家\" : \"专家\" }}\n            </template>\n          </el-table-column>\n          <el-table-column prop=\"sex\" label=\"性别\">\n            <template slot-scope=\"scope\">\n              {{ scope.row.sex == 0 ? \"女\" : \"男\" }}\n            </template>\n          </el-table-column>\n          <el-table-column prop=\"cardId\" label=\"证件号\"> </el-table-column>\n          <el-table-column prop=\"department.name\" label=\"所属部门\">\n          </el-table-column>\n          <el-table-column prop=\"address\" label=\"住址\"> </el-table-column>\n          <el-table-column label=\"操作\" width=\"180px\">\n            <template slot-scope=\"scope\">\n              <el-tooltip\n                class=\"item\"\n                effect=\"dark\"\n                content=\"修改\"\n                placement=\"top\"\n              >\n                <el-button\n                  type=\"primary\"\n                  icon=\"el-icon-edit\"\n                  @click=\"editDailogMethod(scope.row.id,scope.row.type,scope.row.address)\"\n                ></el-button>\n              </el-tooltip>\n              <el-tooltip\n                class=\"item\"\n                effect=\"dark\"\n                content=\"删除\"\n                placement=\"top\"\n              >\n                <el-button\n                  type=\"primary\"\n                  icon=\"el-icon-delete-solid\"\n                  @click=\"deleteDocket(scope.row.user.id)\"\n                ></el-button>\n              </el-tooltip>\n              <!--编辑弹出层  -->\n              <el-dialog title=\"编辑信息\" :visible.sync=\"editDailog\">\n                <el-form :model=\"Doctor\">\n                  <el-form-item label=\"昵称\">\n                    <el-input :value=\"scope.row.name\"  autocomplete=\"off\" disabled></el-input>\n                  </el-form-item>\n                  <el-form-item label=\"是否为专家\">\n                    <el-switch  v-model=\"Doctor.type\"  active-color=\"#13ce66\"  inactive-color=\"#ff4949\"  >\n                    </el-switch>\n                  </el-form-item>\n                  <el-form-item label=\"住址\">\n                    <el-input  v-model=\"Doctor.address\"  autocomplete=\"off\"  ></el-input>\n                  </el-form-item>\n                  <el-button type=\"primary\" @click=\"editDoctor\" >确认修改</el-button >\n                  <el-button type=\"info\" @click=\"editDailog = false\" >取消</el-button >\n                </el-form>\n              </el-dialog>\n            </template>\n          </el-table-column>\n        </el-table>\n           <!-- 添加弹出层 -->\n           <el-dialog title=\"添加医生\" :visible.sync=\"addDailog\">\n              <el-form :model=\"addDoctor\">\n                <el-form-item label=\"昵称\">\n                  <el-input\n                    v-model=\"addDoctor.name\"\n                    autocomplete=\"off\"\n                  ></el-input>\n                </el-form-item>\n                <el-form-item label=\"是否为专家\">\n                  <el-switch\n                    v-model=\"addDoctor.type\"\n                    autocomplete=\"off\"\n                  ></el-switch>\n                </el-form-item>\n                <el-form-item label=\"性别\">\n                  <el-radio v-model=\"radio\" label=\"0\">女</el-radio>\n                  <el-radio v-model=\"radio\" label=\"1\">男</el-radio>\n                </el-form-item>\n                <el-form-item label=\"证件号\">\n                  <el-input\n                    v-model=\"addDoctor.cardId\"\n                    autocomplete=\"off\"\n                  ></el-input>\n                </el-form-item>\n                <el-form-item label=\"所属部门\">\n                  <el-select\n                    v-model=\"addDoctor.department\"\n                    placeholder=\"请选择部门\"\n                  >\n                    <el-option label=\"区域一\" value=\"shanghai\"></el-option>\n                    <el-option label=\"区域二\" value=\"beijing\"></el-option>\n                  </el-select>\n                </el-form-item>\n                <el-form-item label=\"住址\">\n                  <el-input\n                    v-model=\"addDoctor.address\"\n                    autocomplete=\"off\"\n                  ></el-input>\n                </el-form-item>\n                <el-button type=\"primary\" @click=\"addDoctorMethod\"\n                  >确认添加</el-button\n                >\n                <el-button type=\"info\" @click=\"addDailog = false\"\n                  >取消</el-button\n                >\n              </el-form>\n            </el-dialog>\n      </el-row>\n    </main>\n  </div>\n</template>\n\n<script>\nimport adminTop from \"./admin-top.vue\";\nexport default {\n  components: {\n    adminTop,\n  },\n  data() {\n    return {\n      query: {\n        name: \"\",\n        doctor: \"\",\n      },\n      name:'',\n      radio: \"1\",\n      //添加医生\n      addDoctor: {\n        name: \"\",\n        type: \"\",\n        sex: \"\",\n        cardId: \"\",\n        department: \"\",\n        address: \"\",\n        username: \"\",\n        password: \"\",\n      },\n      // 获取数据\n      tableData: [],\n      Doctor: {\n        type: \"\",\n        address: \"\",\n        id: \"\",\n        user: {\n          id: 0,\n        },\n      },\n      editDailog: false,\n      addDailog: false,\n    };\n  },\n  created() {\n    this.getList();\n  },\n  methods: {\n    getList() {\n      this.$http.get(\"/doctor/queryAll\").then((res) => {\n        this.tableData = res.data;\n        \n      });\n    },\n    deleteDocket(id) {\n      this.Doctor.user.id = id;\n      this.$http.post(\"/doctor/delete\", this.Doctor).then((res) => {\n        this.$message.success(\"删除成功\");\n        return this.getList();\n      });\n      return this.$message.error(\"服务繁忙,稍后重试\");\n    },\n    editDailogMethod(id,type,address){\n          this.editDailog = true\n           this.Doctor.id = id\n           if(type===0){\n              this.Doctor.type = false\n           }else{\n               this.Doctor.type = true\n           }\n           this.Doctor.address = address\n    },\n    //修改用户信息\n    editDoctor() {\n      console.log(this.Doctor.id);\n      if (this.Doctor.type === true) {\n        this.Doctor.type = 1;\n      } else {\n        this.Doctor.type = 0;\n      }\n      console.log(this.Doctor);\n      this.$http.post(\"/doctor/update\", this.Doctor).then((res) => {\n        this.editDailog = false;\n        this.$message.success('修改成功')\n        return this.getList();\n      });\n    },\n    // 添加医生\n    addDoctorMethod\n  },\n};\n</script>\n\n<style scoped lang=\"less\">\nmain {\n  width: 80%;\n  height: 100%;\n  margin: 0 auto;\n  h3 {\n    font-family: \"楷体\";\n    font-weight: 300;\n    color: rgba(0, 0, 0, 0.633);\n  }\n  .el-input {\n    width: 30%;\n  }\n}\n</style>"]},"metadata":{},"sourceType":"module"}