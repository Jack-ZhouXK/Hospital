  <h3>挂号</h3>\r\n        </nav>\r\n      </el-col>\r\n      <el-col :span=\"15\" :offset=\"2\" style=\"margin-top: 20px\">\r\n        <el-form ref=\"form\" label-position=\"left\">\r\n          <el-form-item v-model=\"create.reserveInfo.department.id\" label=\"选择科室\">\r\n            <el-select v-model=\"value\" placeholder=\"请选择科室\" @change=\"checkDep\">\r\n              <el-option :label=\"dep.name\" :value=\"dep.id\" v-for=\"dep in dempartment\" :key=\"dep.id\"></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n          <el-form-item  v-model=\"create.reserveInfo.doctor.id\" label=\"预约医生\">\r\n            <el-select v-model=\"value\" placeholder=\"请选择医生\" @blur=\"checkDoc\">\r\n                <el-option :label=\"f.name\" :value=\"f.id\" v-for=\"f in filter\"  :key=\"f.id\" ></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n          <el-form-item label=\"预约时间\">\r\n            <el-date-picker  type=\"date\"  placeholder=\"选择日期\"  v-model=\"create.reserveInfo.year\"  style=\"width: 150px\" ></el-date-picker>\r\n            ---\r\n            <el-time-picker  placeholder=\"选择时间\"  v-model=\"create.reserveInfo.time\"  style=\"width: 150px\" ></el-time-picker>\r\n          </el-form-item>\r\n          <el-form-item label=\"门诊费用\">\r\n            <el-input v-model=\"create.reserveInfo.price\" placeholder=\"门诊费用(元)\"></el-input>\r\n          </el-form-item>\r\n              <el-button type=\"primary\" style=\"width:70%\" @click=\"addReserveInfo\">立即创建</el-button>\r\n        </el-form>\r\n      </el-col>\r\n    </el-row>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      dempartment:[],\r\n      doctor:[],\r\n      filter:[],\r\n      create:{\r\n        reserveInfo:{\r\n          name:'',\r\n          department:{\r\n            id:0\r\n          },\r\n          doctor:{\r\n            id:0\r\n          },\r\n          patient:{\r\n            id:0\r\n          },\r\n          price:'99',\r\n          year:'',\r\n          time:'',\r\n          flag:0\r\n        },\r\n        userId:0\r\n      },\r\n      value:''\r\n    };\r\n  },\r\n  created(){\r\n      this.loadingData()\r\n  },\r\n  methods: {\r\n    toPageHome() {\r\n      this.$router.push(\"/patientPageHome\");\r\n    },\r\n    outLogin(){\r\n        sessionStorage.clear();\r\n        if(sessionStorage.getItem('username')==null){\r\n             this.$message.success('退出成功')\r\n             return this.$router.push('/PageHome')\r\n        }\r\n    },\r\n    loadingData(){\r\n      this.$http.get('/department/queryAll').then(res=>{\r\n        this.dempartment = res.data;\r\n      })\r\n      this.$http.get('/doctor/queryAll').then(res=>{\r\n        this.doctor = res.data;\r\n      })\r\n    },\r\n    checkDep(val){\r\n      this.filter = []\r\n      this.create.reserveInfo.department.id = val\r\n        let index=0\r\n        for (let key in this.doctor) {\r\n          if(this.doctor[key].department.id === val){\r\n            this.filter[index] = this.doctor[key]\r\n            index++\r\n          }\r\n        }\r\n      this.create.reserveInfo.doctor.id = this.filter.id;\r\n      log\r\n  },\r\n    checkDoc(val){\r\n      console.log('vaaa',val);\r\n      this.create.reserveInfo.doctor.id = val;\r\n    },\r\n    addReserveInfo(){\r\n      this.create.userId = sessionStorage.getItem('userId')\r\n      this.create.reserveInfo.time = this.create.reserveInfo.year + this.create.reserveInfo.time\r\n      console.log(this.create);\r\n      this.$http.post('/reserveInfo/addReserveInfo',this.create).then(res=>{\r\n        console.log(res);\r\n      })\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n.wrap {\r\n  width: 80%;\r\n  margin: 0 auto;\r\n  border: 1px solid red;\r\n  h3 {\r\n    color: rgba(0, 0, 255, 0.432);\r\n    text-align: center;\r\n  }\r\n  nav {\r\n    width: 80%;\r\n    height: auto;\r\n    margin-top: 20px;\r\n    box-shadow: 5px 5px 