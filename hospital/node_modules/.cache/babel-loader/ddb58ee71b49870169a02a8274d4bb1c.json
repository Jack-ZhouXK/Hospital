 >诊断</el-button\r\n            >\r\n            <el-button type=\"text\" @click=\"prescribe = true\">开药</el-button>\r\n             </template>\r\n          </el-table-column>\r\n        </el-table>\r\n      </el-col>\r\n    </el-row>\r\n    <!-- 查看病史弹出层 -->\r\n    <el-dialog title=\"查看病史\" :visible.sync=\"medicalDialog\">\r\n      <el-table :data=\"tableData\">\r\n        <el-table-column\r\n          property=\"date\"\r\n          label=\"既往病史\"\r\n          width=\"150\"\r\n        ></el-table-column>\r\n        <el-table-column\r\n          property=\"name\"\r\n          label=\"患病时间\"\r\n          width=\"200\"\r\n        ></el-table-column>\r\n        <el-table-column property=\"address\" label=\"就诊医生\"></el-table-column>\r\n      </el-table>\r\n    </el-dialog>\r\n    <!-- 诊断弹出层 -->\r\n    <el-dialog title=\"诊断\" :visible.sync=\"diagnoseDialog\">\r\n      <el-form :model=\"patient\">\r\n        <el-form-item label=\"患者名称\">\r\n          <el-input v-model=\"patient.name\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"患者描述\">\r\n          <el-input type=\"textarea\" v-model=\"form.describe\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"diagnoseDialog = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"diagnoseDialog = false\"\r\n          >确 定</el-button\r\n        >\r\n      </div>\r\n    </el-dialog>\r\n    <!-- 开药弹出层 -->\r\n    <el-dialog title=\"开药\" :visible.sync=\"prescribe\">\r\n      <el-form :model=\"patient\">\r\n        <el-form-item label=\"患者名称\">\r\n          <el-input v-model=\"patient.name\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-table :data=\"tableData\">\r\n          <el-table-column\r\n            property=\"date\"\r\n            label=\"药品名称\"\r\n            width=\"150\"\r\n          ></el-table-column>\r\n          <el-table-column label=\"数量\">\r\n             <el-input v-model=\"number\" placeholder=\"请输入内容\">></el-input>\r\n          </el-table-column>\r\n        </el-table>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"prescribe = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"prescribe = false\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      medicalDialog: false,\r\n      diagnoseDialog: false,\r\n      prescribe: false,\r\n      number:0,\r\n      form: {\r\n        name: \"\",\r\n      },\r\n      patient: {\r\n        name: \"\",\r\n        describe: \"\",\r\n      },\r\n      tableData: [ ],\r\n    }\r\n  },\r\n  created(){\r\n    this.getList()\r\n  },\r\n   methods:{\r\n       outLogin(){\r\n        sessionStorage.clear();\r\n        if(sessionStorage.getItem('username')==null){\r\n             this.$message.success('退出成功')\r\n             return this.$router.push('/PageHome')\r\n        }\r\n      },\r\n      getList(){\r\n        this.$http.get('reserveInfo/queryAll').then(res=>{\r\n          this.$message.success('获取成功')\r\n          console.log(res.data);\r\n          return this.tableData = res.data;\r\n        }).catch(res=>{return this.$message.error('获取失败')})\r\n      },\r\n      medicalDialogMethod(id){\r\n        this.medicalDialog = true\r\n        this.$http.get('judgeInfo/queryJudgeInfo?id='+id).then(res=>{\r\n          this.$message.success('success')\r\n        }).catch(res =>{\r\n          \r\n        })\r\n      }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n.wrap {\r\n  width: 80%;\r\n  margin: 0 auto;\r\n  border: 1px solid red;\r\n  h3 {\r\n    color: rgba(0, 0, 255, 0.432);\r\n    text-align: center;\r\n  }\r\n}\r\n.el-input {\r\n  width: 40%;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}