l-input\n                v-model=\"query.condition\"\n                placeholder=\"患者姓名\"\n              ></el-input>\n              <el-button type=\"primary\" icon=\"el-icon-search\" @click=\"queryByName\">搜索</el-button>\n            </el-form-item>\n          </el-form>\n        </el-col>\n        <!-- 数据 -->\n        <el-table :data=\"tableData\" border style=\"width: 100%\">\n          <el-table-column prop=\"name\" label=\"姓名\"></el-table-column>\n          <el-table-column prop=\"sex\" label=\"性别\">\n            <template slot-scope=\"scopes\">\n              {{ scopes.row.sex == 0 ? \"女\" : \"男\" }}\n            </template>\n          </el-table-column>\n          <el-table-column prop=\"flag\" label=\"是否住院\">\n            <template slot-scope=\"scopes\">\n              {{ scopes.row.flag == 0 ? \"未住院\" : \"住院\" }}\n            </template>\n          </el-table-column>\n        </el-table>\n      </el-row>\n    </main>\n  </div>\n</template>\n\n<script>\nimport adminTop from \"./admin-top.vue\";\nexport default {\n  components: {\n    adminTop,\n  },\n  data() {\n    return {\n      query: {\n       condition:''\n      },\n      tableData: [],\n    };\n  },\n  created() {\n    this.getList();\n  },\n  methods: {\n    getList() {\n      this.$http.get(\"/patient/queryAll\").then((res) => {\n        console.log(res.data);\n        this.tableData = res.data;\n      });\n    },\n    //通过名字查询\n    queryByName(){\n        console.log(this.query.condition);\n        this.$http.get('/patient/queryPatientByName?name='+this.query.condition)    }\n  },\n};\n</script>\n\n<style scoped lang=\"less\">\nmain {\n  width: 80%;\n  height: 100%;\n  margin: 0 auto;\n  h3 {\n    font-family: \"楷体\";\n    font-weight: 300;\n    color: rgba(0, 0, 0, 0.633);\n  }\n  .el-input {\n    width: 30%;\n  }\n}\n</style>"]},"metadata":{},"sourceType":"module"}