this.$http.post(\"/user/addDoctor\",JSON.stringify(JSON.parse())).then((res) => {\n        this.editDailog = false;\n        this.$message.success(\"添加成功\");\n        return this.getList();\n      });\n    },\n  },\n};\n</script>\n\n<style scoped lang=\"less\">\nmain {\n  width: 80%;\n  height: 100%;\n  margin: 0 auto;\n  h3 {\n    font-family: \"楷体\";\n    font-weight: 300;\n    color: rgba(0, 0, 0, 0.633);\n  }\n  .el-input {\n    width: 30%;\n  }\n}\n</style>"]},"metadata":{},"sourceType":"module"}