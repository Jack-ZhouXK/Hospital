\" icon=\"el-icon-circle-plus-outline\"\n            >添加</el-button\n          >\n        </el-col>\n        <!-- 数据 -->\n        <el-table :data=\"tableData\" border style=\"width: 100%\">\n          <el-table-column prop=\"name\" label=\"序号\" width=\"80px\">\n            <template slot-scope=\"scopes\">\n              {{ scopes.$index + 1 }}\n            </template>\n          </el-table-column>\n          <el-table-column prop=\"name\" label=\"药品名称\"></el-table-column>\n          <el-table-column prop=\"num\" label=\"药品数量\"> </el-table-column>\n          <el-table-column prop=\"price\" label=\"药品价格(元)\"></el-table-column>\n          <el-table-column prop=\"address\" label=\"操作\" width=\"180px\">\n            <el-tooltip\n              class=\"item\"\n              effect=\"dark\"\n              content=\"修改\"\n              placement=\"top\"\n            >\n              <el-button type=\"primary\" icon=\"el-icon-edit\"></el-button>\n            </el-tooltip>\n            <template slot-scope=\"scopes\">\n              <el-tooltip\n                class=\"item\"\n                effect=\"dark\"\n                content=\"删除\"\n                placement=\"top\"\n              >\n                <el-button\n                  type=\"primary\"\n                  icon=\"el-icon-delete-solid\"\n                  @click=\"deleteById()\"\n                ></el-button>\n              </el-tooltip>\n            </template>\n          </el-table-column>\n        </el-table>\n      </el-row>\n    </main>\n  </div>\n</template>\n\n<script>\nimport adminTop from \"./admin-top.vue\";\nexport default {\n  components: {\n    adminTop,\n  },\n  data() {\n    return {\n      query: {\n        condition: \"\",\n      },\n      tableData: [],\n    };\n  },\n  created() {\n    this.getList();\n  },\n  methods: {\n    getList() {\n      this.$http.get(\"/drugs/queryAll\").then((res) => {\n        console.log(res.data);\n        this.tableData = res.data;\n      });\n    },\n  },\n};\n</script>\n\n<style scoped lang=\"less\">\nmain {\n  width: 80%;\n  height: 100%;\n  margin: 0 auto;\n  h3 {\n    font-family: \"楷体\";\n    font-weight: 300;\n    color: rgba(0, 0, 0, 0.633);\n  }\n  .el-input {\n    width: 30%;\n  }\n}\n</style>"]},"metadata":{},"sourceType":"module"}